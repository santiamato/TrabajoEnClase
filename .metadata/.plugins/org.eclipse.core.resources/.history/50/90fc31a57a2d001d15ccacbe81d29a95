#include <stdio.h>
#include <stdlib.h>

float convertirCelsius(float temp, float *numConvertido){
	int convertidoAFahrenheit;
	convertidoAFahrenheit = (temp * 1.8) + 32;

	return 0;
}

float convertirFahrenheit(float temp, float *numConvertido){
	int convertidoACelsius;
		convertidoACelsius = (temp - 32) * 0.5556;

		return 0;
}


